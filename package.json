{
  "name": "prime-reactjs",
  "version": "0.1.1",
  "description": "React Redux projects",
  "scripts": {
    "start": "NODE_ENV=production node server/index.js",
    "start:dev": "NODE_ENV=development node server/index.js",
    "prebuild": "rimraf dist",
    "build": "NODE_ENV=production ./node_modules/.bin/webpack --config ./internals/webpack/webpack.config.js",
    "predeploy": "npm run build",
    "predeploy:staging": "npm run build",
    "deploy": "NODE_ENV=production && (cd ./dist && ../node_modules/.bin/s3-upload ../internals/aws-upload.conf.js)",
    "deploy:staging": "NODE_ENV=staging && (cd ./dist && ../node_modules/.bin/s3-upload ../internals/aws-upload.conf.js)",
    "lint": "./node_modules/.bin/eslint source",
    "lint:fix": "npm run lint -- --fix",
    "release": "./node_modules/.bin/release-it",
    "e2e-setup": "selenium-standalone install",
    "test": "cross-env NODE_ENV=development jest --silent --coverage",
    "test:dev": "cross-env NODE_ENV=development jest --coverage",
    "test:e2e": "nightwatch",
    "test:watch": "cross-env NODE_ENV=development jest --watch"
  },
  "repository": {
    "type": "git"
  },
  "author": "Prime Reactjs",
  "contributors": [
    "Hemerson Vianna <hemerson.lourenco@gmail.com> (http://)"
  ],
  "dependencies": {
    "axios": "0.17.1",
    "babel-polyfill": "6.26.0",
    "dnscache": "1.0.1",
    "ejs": "2.5.8",
    "es6-promise": "4.2.4",
    "express": "4.16.3",
    "history": "4.7.2",
    "i18n-react": "0.6.3",
    "immutable": "3.8.2",
    "moment": "2.22.0",
    "prop-types": "15.6.1",
    "react": "16.2.0",
    "react-csv": "1.0.14",
    "react-date-range": "0.9.4",
    "react-dom": "16.2.0",
    "react-redux": "5.0.7",
    "react-router": "4.2.0",
    "react-router-config": "1.0.0-beta.4",
    "react-router-dom": "4.2.2",
    "react-router-redux": "5.0.0-alpha.9",
    "redux": "3.7.2",
    "redux-immutable": "4.0.0",
    "redux-promise-middleware": "5.0.0",
    "redux-thunk": "2.2.0"
  },
  "devDependencies": {
    "@babel/core": "7.0.0-beta.41",
    "@babel/plugin-proposal-class-properties": "7.0.0-beta.41",
    "@babel/plugin-proposal-export-default-from": "7.0.0-beta.44",
    "@babel/plugin-proposal-export-namespace-from": "7.0.0-beta.44",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0-beta.41",
    "@babel/preset-env": "7.0.0-beta.41",
    "@babel/preset-react": "7.0.0-beta.41",
    "@babel/register": "7.0.0-beta.41",
    "autoprefixer": "7.2.6",
    "babel-eslint": "7.2.3",
    "babel-loader": "8.0.0-beta.2",
    "babel-plugin-module-resolver": "2.7.1",
    "babel-plugin-webpack-alias": "2.1.2",
    "chai": "4.0.2",
    "chai-enzyme": "0.6.1",
    "classnames": "2.2.5",
    "css-loader": "0.28.1",
    "enzyme": "2.9.1",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "4.19.0",
    "eslint-config-google": "0.9.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-prettier": "2.3.1",
    "eslint-plugin-react": "7.5.1",
    "expose-loader": "0.7.5",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.11",
    "html-webpack-plugin": "2.30.1",
    "husky": "0.13.4",
    "identity-obj-proxy": "3.0.0",
    "jsdom": "9.12.0",
    "moxios": "0.3.0",
    "nightwatch": "0.9.15",
    "nock": "9.0.2",
    "nodemon": "1.17.2",
